<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  <remote  name="yocto"
           fetch="git://git.yoctoproject.org"/>

  <default revision="refs/heads/daisy"
           remote="yocto"
           sync-j="4"/>

  <remote  name="oe"
           fetch="git://git.openembedded.org"/>

  <remote  name="github"
           fetch="git://github.com"/>

  <remote  name="linaro"
           fetch="git://git.linaro.org/openembedded"/>

  <!-- Distro Layer -->
  <project name="OpenEneaLinux/meta-oel" remote="github" path="meta-oel"/>

  <!-- Core components -->
  <project name="poky" remote="yocto" path="poky"/>
  <project name="meta-openembedded" remote="oe" path="meta-openembedded"/>

  <project name="meta-selinux" remote="yocto" path="meta-selinux" revision="refs/heads/master"/>

  <!-- Application components -->
  <project name="meta-virtualization" remote="yocto" path="meta-virtualization"/>
  <project name="meta-realtime" remote="yocto" path="meta-realtime" revision="refs/heads/master"/>
  <!-- FIXME: Removed meta-lsi due parse time systemd dependency -->
  <!-- <project name="meta-ivi" remote="yocto" path="meta-ivi"/> -->
  <project name="meta-cloud-services" remote="yocto" path="meta-cloud-services" revision="refs/heads/master"/>
  <project name="meta-dlna" remote="yocto" path="meta-dlna" revision="refs/heads/master"/>
  <project name="meta-security" remote="yocto" path="meta-security" revision="refs/heads/master"/>

  <project name="woglinde/meta-java" remote="github" path="meta-java" revision="refs/heads/master"/>
  <project name="meta-qt5/meta-qt5" remote="github" path="meta-qt5"/>
  <project name="joaohf/meta-erlang" remote="github" path="meta-erlang" revision="refs/heads/master"/>

  <!-- BSP components -->
  <project name="meta-ti" remote="yocto" path="meta-ti"/>
  <project name="meta-fsl-ppc" remote="yocto" path="meta-fsl-ppc"/>
  <project name="meta-fsl-arm" remote="yocto" path="meta-fsl-arm"/>
  <project name="meta-intel" remote="yocto" path="meta-intel"/>

  <!-- FIXME: Add daisy branch when maintainers awake from slumber -->
  <project name="meta-xilinx" remote="yocto" path="meta-xilinx" revision="refs/heads/master"/>
  <project name="meta-minnow" remote="yocto" path="meta-minnow" revision="refs/heads/master"/>
  <project name="meta-baryon" remote="yocto" path="meta-baryon" revision="refs/heads/master"/>
  <project name="beagleboard/meta-beagleboard" remote="github" path="meta-beagleboard" revision="refs/heads/master"/>
  <project name="OSSystems/meta-browser" remote="github" path="meta-browser" revision="refs/heads/master"/>

  <!-- FIXME: Removed meta-lsi due to parse errors -->
  <project name="meta-darwin" remote="yocto" path="meta-darwin"/>
  <project name="meta-raspberrypi" remote="yocto" path="meta-raspberrypi"/>
  <project name="meta-linaro" remote="linaro" path="meta-linaro"/>

</manifest>
