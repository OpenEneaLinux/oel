  <project name="meta-dlna" remote="yocto" path="meta-dlna" revision="refs/heads/master"/>
  <!-- <project name="meta-eca" remote="yocto" path="meta-eca"/> -->
  <project name="meta-security" remote="yocto" path="meta-security" revision="refs/heads/master"/>

  <project name="woglinde/meta-java" remote="github" path="meta-java" revision="refs/heads/master"/>
  <project name="meta-qt5/meta-qt5" remote="github" path="meta-qt5"/>
  <project name="OSSystems/meta-browser" remote="github" path="meta-browser"/>
  <project name="joaohf/meta-erlang" remote="github" path="meta-erlang" revision="refs/heads/master"/>
  <project name="beagleboard/meta-beagleboard" remote="github" path="meta-beagleboard"/>


